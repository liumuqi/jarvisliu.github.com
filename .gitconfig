[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[user]
	name = muqi.lmq
	email = muqi.lmq@autonavi.com
[diff]
    ;tool = kdiff3
    tool = sourcetree
[difftool]
    prompt = false
[alias]
    c = commit
    st = status
	d = difftool
    lg = log --graph --abbrev-commit --decorate --author-date-order --abbrev-commit --format=format:'%C(bold blue)%h%C(reset) - %an%C(reset) - ''%C(white)%s%C(reset) %C(dim white) - %C(bold cyan)%aD%C(reset) %C(reset)%C(bold yellow)%d%C(reset)' --author='^((?!aone).*)$'
    ;lg = log --graph --abbrev-commit --decorate --author-date-order --abbrev-commit  --format=format:'%C(bold cyan)%ai[%ci]%C(reset) - %C(bold blue)%h%C(reset) - %an%C(reset) - ''%C(white)%s%C(reset) %C(dim white) - %C(bold cyan)%ar%C(reset) - %C(reset)%C(bold yellow)%d%C(reset)' --author='^((?!aone).*)$'
    lpg = log --graph --abbrev-commit --decorate --author-date-order --abbrev-commit --format=format:'%C(bold cyan)%ai %C(reset) - %C(bold blue)%h%C(reset) - %an%C(reset) - ''%C(white)%s%C(reset) %C(dim white) - %C(bold cyan)%ar%C(reset) %C(reset)%C(bold yellow)%d%C(reset) ' --first-parent --author='^((?!aone).*)$'
    lsg = log --graph --abbrev-commit --decorate --author-date-order --abbrev-commit --format=format:'%C(bold cyan)%ai %C(reset) - %C(bold blue)%h%C(reset) - %an%C(reset) - ''%C(white)%s%C(reset) %C(dim white) - %C(bold cyan)%ar%C(reset) %C(reset)%C(bold yellow)%d%C(reset) ' --all --author='^((?!aone).*)$';--simplify-by-decoration
    lspg = log --graph --abbrev-commit --decorate --author-date-order --abbrev-commit --format=format:'%C(bold cyan)%ai %C(reset) - %C(bold blue)%h%C(reset) - %an%C(reset) - ''%C(white)%s%C(reset) %C(dim white) - %C(bold cyan)%ar%C(reset) %C(reset)%C(bold yellow)%d%C(reset)' --all --first-parent --author='^((?!aone).*)$'
    rl = reflog --pretty=reflog
[color]
    diff = auto
	ui = true
[core]
	excludesfile = /Users/qishan/.gitignore_global
	ignorecase = false
	;editor = vim
[merge]
    ;tool = kdiff3
    tool = sourcetree
	conflictstyle = diff3
	commit = no
	ff = no
    log = true
;[difftool "sourcetree"]
	;cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	;path = 
;[mergetool "sourcetree"]
	;cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	;trustExitCode = true
[credential]
	helper = osxkeychain
[pull]
    rebase = true
;[mergetool]
	;keepBackup = false
[grep]
	patternType = perl
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/qishan/.stCommitMsg
[pretty]
    reflog = %C(auto)%h %<|(20)%gd %C(red)%cr%C(reset) %gs (%s)
